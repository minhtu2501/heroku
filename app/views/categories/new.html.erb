<% provide(:title, "Category") %>
<h1>Create Category </h1>	
<div class = "row">
	<div class = "col-md-6 col-md-offset-3">
		<%= form_for @category do |f| %>
		<%= f.label :name %>
		<%= f.text_field :name, class: 'form-control'%>
		<%= f.submit 'Create', class:'btn btn-control' %>
		<% end %>
	</div>
</div>
